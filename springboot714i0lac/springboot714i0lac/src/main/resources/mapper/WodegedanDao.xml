<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.dao.WodegedanDao">

	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.entity.WodegedanEntity" id="wodegedanMap">
        <result property="gedanmingcheng" column="gedanmingcheng"/>
        <result property="songname" column="songname"/>
        <result property="gequfenlei" column="gequfenlei"/>
        <result property="fengmian" column="fengmian"/>
        <result property="singer" column="singer"/>
        <result property="songfile" column="songfile"/>
        <result property="zhuanji" column="zhuanji"/>
        <result property="yuyan" column="yuyan"/>
        <result property="banquanfang" column="banquanfang"/>
        <result property="gecidayi" column="gecidayi"/>
        <result property="crossuserid" column="crossuserid"/>
        <result property="crossrefid" column="crossrefid"/>
        <result property="userid" column="userid"/>
    </resultMap>

	<select id="selectListVO"
		resultType="com.entity.vo.WodegedanVO" >
		SELECT * FROM wodegedan  wodegedan         
        <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	
	<select id="selectVO"
		resultType="com.entity.vo.WodegedanVO" >
		SELECT  wodegedan.* FROM wodegedan  wodegedan 	
 		<where> 1=1 ${ew.sqlSegment}</where>
	</select>

    <select id="selectListView"
		resultType="com.entity.view.WodegedanView" >

		SELECT  wodegedan.* FROM wodegedan  wodegedan 	        
        <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	
	<select id="selectView"
		resultType="com.entity.view.WodegedanView" >
		SELECT * FROM wodegedan  wodegedan <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	


</mapper>
